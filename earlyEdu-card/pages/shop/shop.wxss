/* 头部店铺分享评分 */
.header{
  height: 90rpx;
  background: #ffffff;
  width: 690rpx;
  padding: 50rpx 30rpx 30rpx 30rpx;
  display: flex
}
.header_item{
 flex:1
}
.star_img{
  width: 28rpx;
  height: 28rpx;
