}
/*底部按钮*/
.bottombox{
  background-color:#fff;
  font-size:32rpx;
  color:#fff;
  display:flex;
  justify-content:center;
  box-sizing:border-box;
  position:fixed;
  bottom:0;
  width:100%;
  z-index:300;
  height:100rpx;
  align-items: center;
  padding: 0 30rpx
}
view.bottomleftbox{
  flex:0 0 160rpx;
  text-align:center;
  font-size:28rpx;
  border-top:2rpx solid #ddd;
  position:relative;
}
view.bottomleftbox image{
  width:44rpx;
  height:44rpx;
  position:relative;
  top:27rpx;
}
view.bottomleftbox text{
  display:block;
  color:#333;
}
.bottomrightbox{
  background-color:#FF5339;
  flex:1;
  color: #ffffff;
  border-radius: 0 200rpx 200rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0;
  height: 72rpx
}
 .page{
  font-family: "microsoftyahei"
} 
view.page:after{
  content:"";
  display:block;
  clear:both;
  height:100rpx;
}
.detailmodalbox{
  width:540rpx;
}
.detailmodal{
  padding:45rpx 60rpx 50rpx 60rpx;
}

