flex: 0 0 15%;
background:#ffffff;
font-size:22rpx;
height: 42rpx;
color:#333333;
}
.pagesharebtn::after{
  border: none
}
.return{
  width:100%;
  position:fixed;
  z-index:2;
  text-align:right;
  bottom:27%;
}

/*标题与详情*/
.caredetailbox{
  display:flex;
  margin-top:20rpx;
}
.headline{
  font-size:32rpx;
}
.introdetailbox{
  flex:1;
  font-size:24rpx;
  line-height:36rpx;
  margin-right:25rpx;
  color:#d0d0d0;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:3;
  -webkit-box-orient:vertical;
  letter-spacing:2rpx;
}
.starbox{
  flex:0 0 80rpx;
  height:98rpx;
  border-left:2rpx solid #ddd;
  text-align:center;
}
.starbox image{
  width:45rpx;
  height:45rpx;
}
.savetext{
  font-size:24rpx;
  position:relative;
  top:-7rpx;
  color:#333;
}
/*任务介绍*/
.lineboxlayer{
  display:flex;
   
}
.taskhead{
  width:154rpx;
  text-align:center;
  font-size: 30rpx;
  color:#333;  
}
.linebox{
  flex:1;
  border-bottom:2rpx solid #ddd;
 position:relative;
top:18rpx;
height:4rpx;
}
.dotgrey{
  display:inline-block;
  width:15rpx;
  height:15rpx;
  background-color:#ddd;
  border-radius:50%;
  z-index:100;
  position:absolute;
  top:-2rpx;
}
.dotleft{
  text-align:right;
}
.taskheadtext{
  font-size:36rpx;
  color:#333;
  padding:40rpx 0 0rpx 0;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:1;
  -webkit-box-orient:vertical;
  letter-spacing:2rpx;
  /* border-bottom:2rpx solid #e8e8ed; */
} 
.taskitem {
  border-top:2rpx solid #e8e8ed;
  font-size:28rpx;
  padding:20rpx 10rpx;
  color:#333;
}
view.taskitemone{
  margin-top:26rpx;
}
.taskitemlast{
  border-bottom:2rpx solid #e8e8ed;
}
.taskitem image{
  vertical-align: middle;
  margin-right:10rpx;
}
.taskmainimgbox{
   width:100%;
  height:344rpx; 
  margin-top:0 0 30rpx 0;
}
.taskmainimgbox image{
  width:100%;
  height:100%;
}
